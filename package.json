{
  "name": "react-ui-boilerplate",
  "description": "A react boilerplate with Material-UI, Redux, Typescript and another things...",
  "author": {
    "name": "Max RogÃ©rio",
    "email": "max10rogerio@hotmail.com",
    "url": "https://github.com/max10rogerio"
  },
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/max10rogerio/react-ui-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/max10rogerio/react-ui-boilerplate/issues"
  },
  "engines": {
    "node": ">=10.15.x",
    "yarn": ">=1.20.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build && cra-append-sw ./custom-sw.js",
    "serve": "serve -s build -l 4000",
    "build:serve": "yarn build && yarn serve",
    "test": "react-scripts test --env=jsdom-fourteen --verbose",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/react-redux": "^7.1.9",
    "@types/yup": "^0.29.3",
    "axios": "^0.19.2",
    "history": "^4.10.1",
    "notistack": "^0.9.16",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "typescript": "~3.9.5",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^12.0.7",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "cra-append-sw": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "serve": "^11.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.tsx",
      "src/hooks/**/*.(tsx|ts)",
      "!src/serviceWorker.ts",
      "!src/react-app-env.d.ts",
      "!src/index.tsx"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 120
  },
  "eslintConfig": {
    "globals": {
      "process": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "prettier"
    ],
    "env": {
      "browser": true,
      "jasmine": true,
      "jest": true,
      "es6": true
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/ban-types": "off",
      "no-console": "error",
      "react/prop-types": "off",
      "react/display-name": "off",
      "prettier/prettier": [
        "error"
      ]
    }
  }
}
